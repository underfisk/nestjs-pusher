is source tree.\n */\nimport React, { useState, useCallback } from 'react';\nimport Link from '@docusaurus/Link';\nimport isInternalUrl from '@docusaurus/isInternalUrl';\nimport { NavItem, Nav, Button } from 'reactstrap';\nimport useLockBodyScroll from '@theme/hooks/useLockBodyScroll';\nimport classNames from 'classnames';\nimport styles from './styles.module.css';\n\nconst DocSidebarItem = ({\n  item,\n  onItemClick\n}) => {\n  if (item.type === 'category') {\n    return item.items.length > 0 ? <div>\n        <h4 className=\"ml-2\">{item.label}</h4>\n        {item.items.map(childItem => <DocSidebarItem key={childItem.label} item={childItem} onItemClick={onItemClick} />)}\n      </div> : <></>;\n  } else if (item.type === 'link') {\n    return <NavItem>\n        <Link key={item.label} className=\"sidebar-item m-4 text-white\" to={item.href} {...isInternalUrl(item.href) ? {\n        isNavLink: true,\n        activeClassName: 'active',\n        exact: true,\n        onClick: onItemClick\n      } : {\n        target: '_blank',\n        rel: 'noreferrer noopener'\n      }}>\n          {item.label}\n        </Link>\n      </NavItem>;\n  } else {\n    return <></>;\n  }\n};\n\nconst DocSidebar = ({\n  sidebar\n}) => {\n  const [sidebarShown, setSidebarShown] = useState(false);\n  const handleSidebarToggle = useCallback(() => {\n    setSidebarShown(!sidebarShown);\n  }, [sidebarShown, setSidebarShown]);\n  useLockBodyScroll(sidebarShown);\n  return <div className={classNames('bg-info', styles.sidebar)}>\n      <div className={classNames('text-white', {\n      [styles.isOpen]: sidebarShown\n    })}>\n        <div className=\"d-flex w-100 justify-content-end mr-5\">\n          <Button color=\"secondary\" onClick={handleSidebarToggle} className={classNames('mr-2', styles.sidebarFAB)}>\n            <svg aria-label=\"Menu\" xmlns=\"http://www.w3.org/2000/svg\" height={24} width={24} viewBox=\"0 0 32 32\" role=\"img\" focusable=\"false\">\n              <title>Menu</title>\n              <path stroke=\"currentColor\" strokeLinecap=\"round\" strokeMiterlimit=\"10\" strokeWidth=\"2\" d=\"M4 7h22M4 15h22M4 23h22\" />\n            </svg>\n          </Button>\n        </div>\n        <div className={classNames(styles.sideMenu)}>\n          <Nav vertical className=\"list-unstyled p-3 mr-auto\">\n            {sidebar.map(item => <DocSidebarItem key={item.label} item={item} onItemClick={e => {\n            e.currentTarget.blur();\n            setSidebarShown(false);\n          }} />)}\n          </Nav>\n        </div>\n      </div>\n    </div>;\n};\n\nexport default DocSidebar;"]}]}